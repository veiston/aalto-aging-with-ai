{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/_block/button/button.js", "src/components/_block/input/input.jsx", "src/components/_block/card/card.jsx", "src/components/_block/media/media.jsx", "src/components/_block/plate/plate.jsx", "src/components/_block/textarea/textarea.jsx", "src/components/_block/plate-wide/plate-wide.jsx", "src/components/hero/hero.jsx", "src/components/workflow/workflow.jsx", "src/components/about/about.jsx", "src/components/contact/contact.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
