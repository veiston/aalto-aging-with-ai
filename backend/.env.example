## Example .env for aalto-aging-with-ai
# Copy this to `.env` and fill values locally (DO NOT commit real secrets).

# Twilio credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Google Cloud (either set path to service account JSON or API key)
# Preferred: set path to service account JSON and set GOOGLE_APPLICATION_CREDENTIALS to that path
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
GOOGLE_API_KEY=your_google_api_key

# OpenAI / ElevenLabs keys
OPENAI_API_KEY=your_openai_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Local config
FLASK_ENV=development
DEBUG=true
